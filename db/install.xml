<?xml version="1.0" encoding="UTF-8" ?>
<XMLDB xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" PATH="blocks/teacher_tours/db" VERSION="20250103"
       COMMENT="XMLDB file for Moodle blocks/teacher_tours"
       xsi:noNamespaceSchemaLocation="../../../lib/xmldb/xmldb.xsd">
    <TABLES>
        <TABLE NAME="block_teacher_tours" COMMENT="Raw records for custom tours">
            <FIELDS>
                <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="true"/>
                <FIELD NAME="rawdata" TYPE="text" NOTNULL="false" SEQUENCE="false" COMMENT="Records with custom flag"/>
                <FIELD NAME="placementid" TYPE="text" NOTNULL="false" SEQUENCE="false" COMMENT="Placement id eg sectionid or header"/>
                <FIELD NAME="courseid" TYPE="int" LENGTH="20" NOTNULL="true" SEQUENCE="false" COMMENT="Course id"/>
            </FIELDS>
            <KEYS>
                <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
            </KEYS>
        </TABLE>
    </TABLES>
</XMLDB>
